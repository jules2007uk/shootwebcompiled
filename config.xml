<?xml version="1.0" encoding="UTF-8"?>
<widget 
xmlns = "http://www.w3.org/ns/widgets"
xmlns:gap = "http://phonegap.com/ns/1.0"
id = "Sticky Balls"
version = "0.0.1">

<!--<supports-screens android:anyDensity="true" android:resizeable="true" 
    android:smallScreens="true" 
    android:normalScreens="true" 
    android:largeScreens="true" 
    android:xlargeScreens="true" />-->

<name>Sticky Balls</name>
<description></description>
<preference name="phonegap-version" value="3.4.0" />

<!-- Allow connectivity to scoreboard API -->
<allow-navigation href="http://*/*" />

<!-- sample preference specifications -->
<preference name="autorotate" value="false" readonly="true"/>
<preference name="orientation" value="portrait" />
<preference name="fullscreen" value="true" />

<!-- icons -->
<icon src="images/icon-mdpi.png" platform="android" qualifier="mdpi" />
<icon src="images/icon-hdpi.png" platform="android" qualifier="hdpi" />
<icon src="images/icon-xhdpi.png" platform="android" qualifier="xhdpi" />
<icon src="images/icon-xxhdpi.png" platform="android" qualifier="xxhdpi" />
<icon src="images/icon-xxxhdpi" platform="android" qualifier="xxxhdpi" />
<icon src="images/icon-xxxhdpi" platform="android" qualifier="fr-xxhdpi" />

<!-- Platforms: Customize as needed. -->
<gap:platforms>
   <gap:platform name="android" minVersion="2.3" />
</gap:platforms>
</widget>